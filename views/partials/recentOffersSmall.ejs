<div class="container">
    <div class="media">
      <li class="media">
       <div class="media-left">
        <a href="#">
          <img   src="/images/Download.png" height=64em width=64em>
            <% if(offer.getImages().length > 0) {
              var firstImage = offer.getImages()[0]; %>
                <img src="<%= firstImage.getUrl() %>" height=64em width=64em>
            <% } %>
        </a>
       </div>
        <div class="media-body">
          <h4 class="media-heading" id="title"><%= offer.getTags()%></h4>
          <p><%= i18n('Price')%>:<%= offer.getPrice() %> <span class="glyphicon glyphicon-euro" aria-hidden="true"/></p>
          <%- partial ('../partials/crudButtonsOffer.ejs',{offer: offer}) %>
        </div>
      </li>
    </div>
</div>



