<div class="crudbtn media-right btn-group-vertical" role="group">
  <% if (DemandService.belongsToCurrentUser(req, demand)) { %>
    <a href="<%= DemandService.getEditUrl(demand) %>"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a>

  <a class="showYesNoDialog"
     data-yesActionUrl="<%= DemandService.getDeleteUrl(demand) %>"
     data-title="<%- i18n('Delete demand?') %>"
     data-body="<%- i18n('Are you sure to delete this demand? This cannot be undone.') %>"
     data-yesActionLabel="<%- i18n('Delete') %>"
     data-noActionLabel="<%- i18n('Cancel') %>">
    <span class="glyphicon glyphicon-remove" aria-hidden="true"/></a>
  
  <a href="<%= DemandService.getMatchingUrl(demand) %>"><span class="glyphicon glyphicon-check" aria-hidden="true"></span></a>
  <% } %>
</div>
