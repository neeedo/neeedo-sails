<li class="demands ">
  <% // I'm actually not sure why we need to unserialize again, but the following line is really important when this partial is used during an AJAX action, do not remove!                %>
  <% demand = ApiClientService.newDemand().loadFromSerialized(demand) %>

  <div class="row">
    <div class="col-xs-5">

      <label for="mustTags"><%= i18n('Must have`s') %></label>

      <ul id="mustTags">
        <% demand.getMustTags().forEach(function(mustTags){ %>
        <li>
          <%= mustTags %>
        </li>
        <% }) %>
      </ul>

    </div>
    <div class="col-xs-5">
      <label for="shouldTags"><%= i18n('Should have`s') %></label>

      <ul id="shouldTags">
        <% demand.getShouldTags().forEach(function(shouldTags){ %>
        <li>
          <%= shouldTags %>
        </li>
        <% }) %>
      </ul>
    </div>
    <div class="col-xs-1">
      <%- partial('../partials/crudButtonsDemands.ejs', {demand: demand}) %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-5">

      <label for="dist"><%= i18n('Distance') %>:
      </label>
      <span id="dist"><%= demand.getDistance() %> km</span>

    </div>
    <div class="col-xs-5">
      <label for="price"><%= i18n('Price') %>:</label>
      <span id="price"><%= demand.getPrice().getMin() %> - <%= demand.getPrice().getMax() %><span
          class="glyphicon glyphicon-euro" aria-hidden="true"></span></span>

    </div>
  </div>

</li>
