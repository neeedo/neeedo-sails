
<div class="container">

    <div class="media">
      <div class="media-left">
        <a href="#">
        <img   src="/images/Download.png" height=64em width=64em>
        </a>
      </div>
      <div class="media-body">
        <h4 class="media-heading"><p><%= offer.getTags() %></p></h4>
        <p><%= i18n('Price')%>:<%= offer.getPrice() %> <span class="glyphicon glyphicon-euro" aria-hidden="true"/></p>
        <% if (OfferService.belongsToCurrentUser(req, offer)) { %>
        <a href="<%= OfferService.getEditUrl(offer) %>"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a>
        <a href="<%= OfferService.getDeleteUrl(offer) %>"><span class="glyphicon glyphicon-remove" aria-hidden="true"/></a>
        <% } %>
      </div>
    </div>


</div>
