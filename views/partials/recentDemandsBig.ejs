<div class="container">

  <div class="media">
    <li class="media">
      <div class="media-left">
      </div>
      <div class="media-body">
        <h4 class="media-heading" id="title"><%= demand.getMustTags() %></h4>
        <p><%= demand.getShouldTags() %></p>
        <p><%= i18n('Distance')%>:<%= demand.getDistance() %></p>
        <p><%= i18n('Price')%>:<%= demand.getPrice() %><span class="glyphicon glyphicon-euro" aria-hidden="true"/></p>
        <% if (DemandService.belongsToCurrentUser(req, demand)) { %>
        <a href="<%= DemandService.getEditUrl(demand) %>"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        </a>
        <a href="<%= DemandService.getDeleteUrl(demand) %>"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
        </a>
        <% } %>
      </div>
    </li>
  </div>

</div>
