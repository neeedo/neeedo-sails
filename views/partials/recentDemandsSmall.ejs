<div class="container">
  <div class="media">
    <li class="media">
      <div class="media-left">
      </div>
      <div class="media-body">
        <h4 class="media-heading" id="title"><%= demand.getMustTags() %></h4>
        <p><%= demand.getShouldTags() %></p>
        <p><%= i18n('Distance')%>:<%= demand.getDistance() %></p>
        <p><%= i18n('Price')%>:<%= demand.getPrice().getMin() %> - <%= demand.getPrice().getMax() %><span class="glyphicon glyphicon-euro" aria-hidden="true"/></p>
        <%- partial ('../partials/crudButtonsdemands.ejs',{demand: demand}) %>
      </div>
    </li>
  </div>
</div>
